		/ Preloader Script
		:javascript
			//
				$(window).load(function() {
					$('.loader').fadeOut();
					$('#preloader').delay(400).fadeOut('slow');
					$('body').delay(400).removeClass('noscroll');
				})
			//
	%body.noscroll
		#preloader
			.loader
				.pacman
					- 3.times do
						%div
		.opl
			%a{:href => "http://onepagelove.com/oleg-zayarniy", :target => "_blank"} Doteatdot.com featured on One Page Love
		%section.intro
			.intro-copy
				.wrapper
					%ul.pacmen
						- 3.times do
							%li
					%h1
						Hello! I'm Oleg, a digital designer and coder based in Berlin.
			.arrow
		%section.projects
			.projects-stage
				%a.project-preview.hj{"data-modal-target" => "hj"}
					%figure.project-figure
						.project-tmb-overlay
						%img{:src => "dist/img/projects/hj-tmb.png"}
						%figcaption.project-caption
							%h2 Headjet.net
							%p Website &middot; Webapp
				%a.project-preview.bos{"data-modal-target" => "bos"}
					%figure.project-figure
						.project-tmb-overlay
						%img{:src => "dist/img/projects/bos-tmb.png"}
						%figcaption.project-caption
							%h2 Orangutan.de
							%p Website Relaunch
				%a.project-preview.fp{"data-modal-target" => "fp"}
					%figure.project-figure
						.project-tmb-overlay
						%img{:src => "dist/img/projects/fp-tmb.png"}
						%figcaption.project-caption
							%h2 Fitnesspoint App
							%p Website &middot; Webapp
				%a.project-preview.savry{"data-modal-target" => "savry"}
					%figure.project-figure
						.project-tmb-overlay
						%img{:src => "dist/img/projects/savry-tmb.png"}
						%figcaption.project-caption
							%h2 Savry.de
							%p Webapp
		%section.content.info
			.wrapper
				.info-block
					%h2 Clients
					%ul
						%li KPMG
						%li Lichtblick
						%li mybet
						%li McDonalds
						%li BMW
				.info-block
					%h2 Services
					%ul
						%li Digital Design
						%li Information Architecture
						%li Interface Design
						%li Art Direction 
						%li Front-end
				.info-block
					%h2 Tools
					%ul
						%li Adobe CC
						%li Sketch
						%li HTML5
						%li CSS3/SCSS
						%li Javascript
		%section.content.contact
			%a.button-slide{:href => "mailto:oleg@doteatdot.com", "data-hover" => "oleg@doteatdot.com"}
				%span
					Get in touch
		%footer
			.wrapper
				%ul
					-#%li
						%a{"data-modal-target" => "legal"} Legal
					%li
						%a{:href =>"https://github.com/hypersnob/doteatdot", "target" => "_blanc"} Source
					%li
						%a{:href =>"https://www.linkedin.com/in/olegzayarniy", "target" => "_blanc"} Linkedin
		%h1.notification Sorry, this website don't support your browser.
		.over.hj{"data-modal-id" => "hj"}
			.close{"data-modal-action" => "close"}
			.wrapper
				%h1 Headjet.net
				%p
					Headjet is a Berlin based start-up, co-founded by me that helps Western firms to find IT specialists in Eastern Europe and vice versa. My part was product design and front-end development.
				%p
					%span.label Logo
					%span.label Design
					%span.label Front-end
					%span.label Bootstrap 4
				.stage
					%img{:src => "dist/img/projects/hj-home.jpg"}
		.over.bos{"data-modal-id" => "bos"}
			.close{"data-modal-action" => "close"}
			.wrapper
				%h1 Orangutan.de
				%p
					I have designed a new responsive website for non-profit organization BOS Deutschland e.V. which supports rescue projects for orangutans and rainforest conservation projects in Borneo.
				%p
					%span.label Design
					%span.label UX
				.stage
					%img{:src => "dist/img/projects/bos-home.jpg"}
		.over.fp{"data-modal-id" => "fp"}
			.close{"data-modal-action" => "close"}
			.wrapper
				%h1 Fitnesspoint App
				%p
					Fitness Point is a successful app to track your progress at the gym.
					%br
						I've designed and coded a new responsive landing page and the web version of the app.
				%p
					%span.label Design
					%span.label Front-end
					%span.label Zurb Foundation
				.stage
					%img{:src => "dist/img/projects/fp-home.jpg"}
		.over.savry{"data-modal-id" => "savry"}
			.close{"data-modal-action" => "close"}
			.wrapper
				%h1 Savry.de
				%p
					Savry.de <del>is</del> was a social recommendation tool for favorite things and gift ideas available at Amazon. As a co-founder I took over many tasks from conception and product design to front-end development.
				%p
					%span.label Logo
					%span.label Design
					%span.label Front-end
					%span.label UX
				.stage
					%img{:src => "dist/img/projects/savry-phone.jpg"}
					%img{:src => "dist/img/projects/savry-home.jpg"}
